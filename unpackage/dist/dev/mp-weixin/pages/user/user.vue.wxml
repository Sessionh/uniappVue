<template name="88d93a44">
  <view class="_view data-v-112becea">
    <view class="_view data-v-112becea header {{[isH5Plus ? 'status' : '']}}">
      <view class="_view data-v-112becea userinfo">
        <view class="_view data-v-112becea face">
          <image src="{{userinfo.face}}" class="_image data-v-112becea"></image>
        </view>
        <view class="_view data-v-112becea info">
          <view class="_view data-v-112becea username">{{userinfo.username}}</view>
          <view class="_view data-v-112becea integral">积分:{{userinfo.integral}}</view>
        </view>
      </view>
      <view class="_view data-v-112becea setting">
        <image src="../../static/HM-PersonalCenter/setting.png" class="_image data-v-112becea"></image>
      </view>
    </view>
    <view class="_view data-v-112becea orders">
      <view class="_view data-v-112becea box">
        <view class="_view data-v-112becea label" wx:key="row.name" key="{{row.name}}" hover-class="hover" hover-stay-time="50" bindtap="handleProxy" data-eventid="{{'GLM-0-'+index}}" data-comkey="{{$k}}" wx:for="{{orderTypeLise}}" wx:for-index="index"
          wx:for-item="row">
          <view class="_view data-v-112becea icon">
            <view class="_view data-v-112becea badge" wx:if="{{row.badge>0}}">{{row.badge}}</view>
            <image src="{{'../../static/HM-PersonalCenter/'+row.icon}}" class="_image data-v-112becea"></image>
          </view> {{row.name}} </view>
      </view>
    </view>
    <view class="_view data-v-112becea list" wx:key="list" key="{{list}}" wx:for="{{severList}}" wx:for-index="list_i" wx:for-item="list">
      <view class="_view data-v-112becea li {{[li_i == list.length - 1 ? 'noborder' : '']}}" bindtap="handleProxy" hover-class="hover" hover-stay-time="50" wx:key="li.name" key="{{li.name}}" data-eventid="{{'jmX-1-'+list_i+'-'+li_i}}" data-comkey="{{$k}}"
        wx:for="{{list}}" wx:for-index="li_i" wx:for-item="li">
        <view class="_view data-v-112becea icon">
          <image src="{{'../../static/HM-PersonalCenter/sever/'+li.icon}}" class="_image data-v-112becea"></image>
        </view>
        <view class="_view data-v-112becea text">{{li.name}}</view>
        <image class="_image data-v-112becea to" src="../../static/HM-PersonalCenter/to.png"></image>
      </view>
    </view>
  </view>
</template>